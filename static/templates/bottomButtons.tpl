<div id="ua-magic-button-wrapper" class="position-fixed start-0 end-0 w-100 no-propagation">

        <div id="magic-button-swiper" class="swiper-container">
                <div class="swiper-wrapper">


                        <div class="swiper-slide">


                                <div class="perspective">

                                        <div class="ua-magic-button">

                                                <div class="btn-group w-100" role="group"
                                                        aria-label="First set of Button group">

                                                        {uacmp.magicButtonSlide}



                                                </div>







                                        </div>

                                </div>



                        </div>


                        <div class="swiper-slide">


                                <div class="perspective">
                                        <div class="ua-magic-button">


                                                <div class="btn-group w-100" role="group"
                                                        aria-label="Buttons when cards is opened">

                                                        <button id="rotateCards" class="btn btn-primary btn-magic-start"
                                                                type="button">


                                                                <i class="fa-solid fa-toggle-off"></i> List
                                                        </button>


                                                        <button class="btn btn-primary btn-magic-middle removeCards"
                                                                type="button">
                                                                <i class="fa-solid fa-xmark fs-3"
                                                                        style="color: #fe5d5d;"></i>
                                                        </button>

                                                        <div class="dropdown-center">
                                                                <button class="btn btn-primary btn-magic-end dropdown-toggle"
                                                                        type="button" data-bs-toggle="dropdown"
                                                                        aria-expanded="false">

                                                                        <span id="sortby-label"
                                                                                class="btn-label d-inline-block text-truncate rounded-pill btn-sm">
                                                                                Distance <i
                                                                                        class="fa fa-solid fa-arrow-down-a-z"></i>
                                                                        </span>
                                                                </button>
                                                                <ul class="dropdown-menu">
                                                                        <li>Sort By:</li>
                                                                        <li><a class="dropdown-item ua-sort"
                                                                                        data-ua-sortby="distance"
                                                                                        href="#">Distance</a></li>
                                                                        <li><a class="dropdown-item ua-sort"
                                                                                        data-ua-sortby="latest"
                                                                                        href="#">Latest</a></li>
                                                                        <li><a class="dropdown-item ua-sort"
                                                                                        data-ua-sortby="oldest"
                                                                                        href="#">Oldest</a></li>
                                                                        <li><a class="dropdown-item ua-sort"
                                                                                        data-ua-sortby="events"
                                                                                        href="#">Event Date</a></li>
                                                                        <li><a class="dropdown-item ua-sort"
                                                                                        data-ua-sortby="category"
                                                                                        href="#">Category</a></li>
                                                                        <li> <select id="location-category-filter"
                                                                                        name="categoryfilter"
                                                                                        class="form-select shadow me-2"
                                                                                        aria-label="category"></select>
                                                                        </li>
                                                                </ul>
                                                        </div>


                                                </div>








                                        </div>
                                </div>



                        </div>








                        <div class="swiper-slide">


                                <div class="perspective">
                                        <div class="ua-magic-button">


                                                <div class="btn-group w-100" role="group"
                                                        aria-label="Buttons when cards is opened">

                                                        <button class="btn btn-primary btn-magic-start" type="button">


                                                                Test
                                                        </button>


                                                        <button class="btn btn-primary btn-magic-middle removeCards"
                                                                type="button">
                                                                <i class="fa-solid fa-xmark fs-3"
                                                                        style="color: #ffffff;"></i>
                                                        </button>


                                                        <button class="btn btn-primary btn-magic-end" type="button"
                                                                data-bs-toggle="offcanvas"
                                                                data-bs-target="#map-controls"
                                                                aria-controls="map-controls">
                                                                <i class="fa-solid fa-layer-group"></i>
                                                        </button>
                                                </div>








                                        </div>
                                </div>



                        </div>

                        <div class="swiper-slide">


                                <div class="perspective">
                                        <div class="ua-magic-button">


                                                <div class="btn-group w-100" role="group"
                                                        aria-label="Buttons when cards is opened">

                                                        <button class="btn btn-primary w-100 rounded-pill d-inline-block text-truncate"
                                                                type="button">

                                                                <i
                                                                        class="fa fas fa-solid fa-wand-magic-sparkles fa-beat"></i>
                                                                <span id="text-info-button"
                                                                        class="rounded-pill">...</span>


                                                        </button>



                                                </div>








                                        </div>
                                </div>



                        </div>







                </div>
        </div>





</div>